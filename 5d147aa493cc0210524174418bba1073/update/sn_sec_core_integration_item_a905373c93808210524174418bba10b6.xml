        <validation_script><![CDATA[validate(sysId, values);

function validate(sysId, values) {
    var config = new x_952688_domaintoo.DomainToolsConfiguration().getConfigurationByIntegId(sysId); 	
	var userName = values[config.userName];
	var apiKey = values[config.apiKey];

	if ((apiKey != '') && (userName != '')){
		var response = validateStatus(userName, apiKey);
		var statusCode = response.getStatusCode();

		if (statusCode != 200)
			result = response.getErrorMessage();
	}	
	return result;
}

function validateStatus(userName, apiKey) {
	var baseEndpoint = 'https://api.domaintools.com/v1/iris-investigate/?domain=test.com'+'&api_username='+userName+'&api_key='+apiKey;
	var restMessage = new sn_ws.RESTMessageV2();
	restMessage.setHttpMethod("GET");
	restMessage.setRequestHeader('Accept', 'application/json');
	restMessage.setEndpoint(baseEndpoint);
	//restMessage.setStringParameter('api_key', apiKey);
	//restMessage.setStringParameter('api_username', userName);
	return restMessage.execute();
}]]></validation_script>
